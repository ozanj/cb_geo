	doi = {10.1023/A:1022541506588},
	id = {TN{\_}cdi{\_}proquest{\_}journals{\_}751988276},
	isbn = {0042-0972},
	journal = {The Urban review},
	keywords = {Colleges \& universities; Secondary school students},
	n2 = {This literature review examines the ways in which college and university outreach programs impact Latino (and other minorities) high school students. This article demonstrates that the outreach programs outlined do aid in closing the gap of college enrollment for Latinos. However, a review of three representative programs--AVID, the University of California's Early Academic Outreach Program, and Upward Bound--reveals that the Latino students who exhibit the most need are summarily excluded from participating in the programs, thus leaving them on the margins of the educational system. One of the conclusions of this study is that the problem lies with the political nature of the programs, which causes them to have stringent eligibility criteria.{$[$}PUBLICATION ABSTRACT{$]$}},
	number = {1},
	pages = {43},
